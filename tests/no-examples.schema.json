{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Test-data",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 0,
      "maxLength": 100
    },
    "age": {
      "type": "integer",
      "minimum": 15,
      "maximum": 45
    },
    "email": {
      "type": "string",
      "minLength": 0,
      "maxLength": 100,
      "format": "email"
    },
    "isActive": {
      "type": "boolean"
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string",
          "minLength": 0,
          "maxLength": 100
        },
        "city": {
          "type": "string",
          "minLength": 0,
          "maxLength": 100
        },
        "zipCode": {
          "type": "string",
          "minLength": 0,
          "maxLength": 100
        },
        "coordinates": {
          "type": "object",
          "properties": {
            "lat": {
              "type": "number",
              "minimum": 20.3564,
              "maximum": 61.0692
            },
            "lng": {
              "type": "number",
              "minimum": -111.009,
              "maximum": -37.003
            }
          },
          "required": [
            "lat",
            "lng"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "street",
        "city",
        "zipCode",
        "coordinates"
      ],
      "additionalProperties": false
    },
    "hobbies": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 0,
        "maxLength": 100
      },
      "minItems": 0,
      "maxItems": 100,
      "uniqueItems": true
    },
    "metadata": {
      "type": "object",
      "properties": {
        "createdAt": {
          "type": "string",
          "minLength": 0,
          "maxLength": 100,
          "format": "date-time"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 0,
            "maxLength": 100
          },
          "minItems": 0,
          "maxItems": 100,
          "uniqueItems": true
        }
      },
      "required": [
        "createdAt",
        "tags"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "name",
    "age",
    "email",
    "isActive",
    "address",
    "hobbies",
    "metadata"
  ],
  "additionalProperties": false
}