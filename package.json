{
  "name": "jssb",
  "version": "1.0.0",
  "description": "A TypeScript library and CLI tool to generate JSON schemas from sample JSON data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jssb": "./bin/jssb.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "example": "ts-node examples/example.ts",
    "test": "echo \"No tests configured yet\"",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish:patch": "npm run version:patch && npm publish",
    "publish:minor": "npm run version:minor && npm publish",
    "publish:major": "npm run version:major && npm publish",
    "publish:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "publish:alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha",
    "publish:dry": "npm publish --dry-run"
  },
  "keywords": [
    "json",
    "schema",
    "json-schema",
    "builder",
    "generator",
    "typescript",
    "cli",
    "command-line"
  ],
  "author": "Your Name <me@arifsetyawan.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arifsetyawan/jssb.git"
  },
  "homepage": "https://github.com/arifsetyawan/jssb#readme",
  "bugs": {
    "url": "https://github.com/arifsetyawan/jssb/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "src",
    "bin",
    "README.md",
    "LICENSE"
  ]
}